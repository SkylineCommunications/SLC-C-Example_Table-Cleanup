<Protocol xmlns="http://www.skyline.be/protocol">
	<Name>Skyline Example Table Cleanup</Name>
	<Description>Skyline Example Table Cleanup DataMiner connector</Description>
	<Version>1.0.0.1</Version>
	<IntegrationID>DMS-DRV-9117</IntegrationID>
	<Provider>Skyline Communications</Provider>
	<Vendor>Skyline Example</Vendor>
	<VendorOID>1.3.6.1.4.1.8813.2.1218</VendorOID>
	<DeviceOID>24</DeviceOID>
	<ElementType>Example</ElementType>
	<Type relativeTimers="true">virtual</Type>
	<Display defaultPage="General" pageOrder="General;Config" wideColumnPages=""/>
	<SNMP includepages="true">auto</SNMP>

	<Compliancies>
		<CassandraReady>true</CassandraReady>
		<MinimumRequiredVersion>10.2.0.0 - 12603</MinimumRequiredVersion>
	</Compliancies>

	<Params>
		<Param id="2">
			<Name>AfterStartup</Name>
			<Description>After Startup</Description>
			<Type>dummy</Type>
		</Param>
		<Param id="3">
			<Name>FillTrapsTable</Name>
			<Type>write</Type>
			<Information>
				<Subtext>Fill the traps table with random data.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>1</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type width="110">button</Type>
				<Discreets>
					<Discreet>
						<Display>Fill Traps Table</Display>
						<Value>1</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		<Param id="4" trending="false" save="true">
			<Name>AmountOfRowsToAdd</Name>
			<Description>Amount of Rows to Add</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Amount of rows that have to be added to the traps table.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<DefaultValue>100</DefaultValue>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Rows</Units>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="5" setter="true">
			<Name>AmountOfRowsToAdd</Name>
			<Description>Amount of Rows to Add</Description>
			<Type>write</Type>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Rows</Units>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="98">
			<Name>CleanupTrapsTable</Name>
			<Type>write</Type>
			<Information>
				<Subtext>Cleanup traps table.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>3</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type width="110">button</Type>
				<Discreets>
					<Discreet>
						<Display>Cleanup Traps Table</Display>
						<Value>1</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		<Param id="99">
			<Name>TriggerQaTableCleanup</Name>
			<Description>Trigger QA for Table Cleanup</Description>
			<Type>dummy</Type>
		</Param>
		<Param id="100" trending="true" save="true">
			<Name>CleanupMethod</Name>
			<Description>Cleanup Method</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Here you can choose how the tables have to be cleaned up.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<DefaultValue>0</DefaultValue>
			</Interprete>
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>discreet</Type>
				<Discreets>
					<Discreet>
						<Display>Row Age and Row Count</Display>
						<Value>0</Value>
					</Discreet>
					<Discreet>
						<Display>Row Age</Display>
						<Value>1</Value>
					</Discreet>
					<Discreet>
						<Display>Row Count</Display>
						<Value>2</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		<Param id="101" setter="true">
			<Name>CleanupMethod</Name>
			<Description>Cleanup Method</Description>
			<Type>write</Type>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>discreet</Type>
				<Discreets>
					<Discreet>
						<Display>Row Age and Row Count</Display>
						<Value>0</Value>
					</Discreet>
					<Discreet>
						<Display>Row Age</Display>
						<Value>1</Value>
					</Discreet>
					<Discreet>
						<Display>Row Count</Display>
						<Value>2</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		<Param id="102" trending="true" save="true">
			<Name>MaximumRowAge</Name>
			<Description>Maximum Row Age</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is how long the row is allowed in the table before it's removed.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<DefaultValue>604800</DefaultValue>
			</Interprete>
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>1</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type options="time">number</Type>
			</Measurement>
		</Param>
		<Param id="103" setter="true">
			<Name>MaximumRowAge</Name>
			<Description>Maximum Row Age</Description>
			<Type>write</Type>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>1</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type options="time">number</Type>
			</Measurement>
		</Param>
		<Param id="104" trending="true" save="true">
			<Name>MaximumRowCount</Name>
			<Description>Maximum Row Count</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is how many rows that are allowed before rows are getting removed from the table.
				There is an offset that will delete 20% extra rows.
			Example: If there are 120 rows in the table and the maximum row count is 100, it will delete 40 rows so that there are 80 rows left in the table.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<DefaultValue>500</DefaultValue>
			</Interprete>
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Rows</Units>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>2</Row>
					</Position>
				</Positions>
				<Range>
					<Low>0</Low>
					<High>10000</High>
				</Range>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="105" setter="true">
			<Name>MaximumRowCount</Name>
			<Description>Maximum Row Count</Description>
			<Type>write</Type>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Rows</Units>
				<Positions>
					<Position>
						<Page>Config</Page>
						<Column>0</Column>
						<Row>2</Row>
					</Position>
				</Positions>
				<Range>
					<Low>0</Low>
					<High>10000</High>
				</Range>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="200">
			<Name>Traps</Name>
			<Description>Traps</Description>
			<Type>array</Type>
			<ArrayOptions index="0">
				<ColumnOption idx="0" pid="201" type="retrieved" options=""/>
				<ColumnOption idx="1" pid="202" type="retrieved" options=";save" />
			</ArrayOptions>
			<Information>
				<Subtext>This is the trap table.</Subtext>
			</Information>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>2</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type options="tab=columns:201|0-202|1,width:200-144,sort:STRING-INT|DESC,lines:25,filter:true">table</Type>
			</Measurement>
		</Param>
		<Param id="201" trending="false">
			<Name>TrapsIndex</Name>
			<Description>Index (Traps)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is the key used internally by DataMiner to identify the table entries.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
		<Param id="202" trending="false">
			<Name>TrapsDateTime</Name>
			<Description>Datetime (Traps)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is the datetime when the trap was generated.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<Decimals>8</Decimals>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Decimals>8</Decimals>
			</Display>
			<Measurement>
				<Type options="datetime">number</Type>
			</Measurement>
		</Param>
	</Params>

	<QActions>
